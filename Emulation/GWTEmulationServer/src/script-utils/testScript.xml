<?xml version="1.0" encoding="UTF-8"?>
<script xmlns="http://mobilis.inf.tu-dresden.de/XMLEmulationScript" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://mobilis.inf.tu-dresden.de/XMLEmulationScript XMLEmulationScript.xsd">
	<instance varName="myAppInstance" appNS="my.app" instanceId="1"/>
	<instanceGroup varName="mySecondAppInstanceGroup" appNS="my.second.app" firstInstanceId="1" instanceCount="20"/>
	<start instance="mySecondAppInstanceGroup"/>
	<appCommand instance="mySecondAppInstanceGroup" methodName="doSomething"/>
	<start instance="mySecondAppInstanceGroup"/>
	<appCommand instance="myAppInstance" methodName="doSomethingElse">
	    <parameter>
	        <string>hello</string>
	        <boolean>true</boolean>
	        <int>15</int>
	    </parameter>
	</appCommand>
	<appCommand instance="mySecondAppInstanceGroup" methodName="doSomethingEntirelyElse">
	    <parameter>
	        <double>42.00</double>
	    </parameter>
	</appCommand>
	<wait time="10000"/>
	<stop instance="myAppInstance"/>
	<stop instance="mySecondAppInstanceGroup"/>
</script>