<?xml version="1.0" encoding="UTF-8"?>
<script xmlns="http://mobilis.inf.tu-dresden.de/XMLEmulationScript" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://mobilis.inf.tu-dresden.de/XMLEmulationScript XMLEmulationScript.xsd">
   <setupCommand>
       <instanceVarDeclaration varName="myAppInstance" appNS="my.app" instanceId="1"/>
   </setupCommand>
   <setupCommand>
   		<instanceVarDeclaration varName="mySecondAppInstance" appNS="my.second.app" instanceId="1"/>
   </setupCommand>
   <setupCommand>
   	   <instance>mySecondInstance</instance>
   	   <start/>
   </setupCommand>
   <appCommand>
   		<instance>mySecondAppInstance</instance>
   		<methodName>doSomething</methodName>
   </appCommand>
   <setupCommand>
       <instance>myAppInstance</instance>
       <start/>
   </setupCommand>
   <appCommand>
       <instance>myAppInstance</instance>
       <methodName>doSomethingElse</methodName>
       <parameter>
           <string>hello</string>
           <boolean>true</boolean>
           <int>15</int>
       </parameter>
   </appCommand>
   <appCommand>
       <instance>mySecondAppInstance</instance>
       <methodName>doSomethingEntirelyElse</methodName>
       <parameter>
           <double>42.00</double>
       </parameter>
   </appCommand>
   <setupCommand>
   	   <instance>myAppInstance</instance>
   	   <stop/>
   </setupCommand>
   <setupCommand>
   	   <instance>mySecondAppInstance</instance>
   	   <stop/>
   </setupCommand>
</script>
